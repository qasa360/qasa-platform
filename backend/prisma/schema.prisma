// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Flat {
  id        Int      @id @default(autoincrement())
  name      String
  address   String
  city      String
  country   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
